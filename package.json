{
  "name": "@scaleleap/amazon-advertising-api-sdk",
  "version": "3.3.1",
  "description": "Amazon Advertising API TypeScript and Node.js Unofficial SDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "postversion": "ts-node --transpile-only --pretty utils/bump-version.ts",
    "build": "tsc --build tsconfig.build.json",
    "docs": "typedoc",
    "clean": "rimraf lib",
    "lint": "eslint --ext .js,.ts src test examples",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "api-extractor": "api-extractor run --local --verbose",
    "semantic-release": "npx @scaleleap/semantic-release-config",
    "test:passthrough": "cross-env POLLY_MODE=passthrough npm test -- -t 'polly:passthrough'",
    "dev:refresh-token": "ts-node test/refresh-token.ts"
  },
  "keywords": [
    "amazon advertising",
    "amazon advertising api",
    "sponsored display",
    "sponsored products",
    "sponsored brands"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@scaleleap/amazon-marketplaces": "14.0.0",
    "agentkeepalive": "4.2.0",
    "axios": "0.24.0",
    "client-oauth2": "4.3.3",
    "fp-ts": "2.11.8",
    "http-status-codes": "2.1.4",
    "io-ts": "2.2.16",
    "io-ts-promise": "2.0.2",
    "io-ts-reporters": "1.2.2",
    "io-ts-types": "0.5.16",
    "lazy-get-decorator": "2.2.0",
    "lodash": "4.17.21",
    "ts-error": "1.0.6"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.19.4",
    "@scaleleap/config": "2.0.8",
    "@scaleleap/jest-polly": "1.5.27",
    "@types/jest": "27.0.3",
    "@types/lodash": "4.14.178",
    "@types/node": "14.17.34",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "26.6.3",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "ts-node": "10.4.0",
    "tsconfigs": "4.0.2",
    "typedoc": "0.22.11",
    "typescript": "4.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "@scaleleap/semantic-release-config"
  }
}
